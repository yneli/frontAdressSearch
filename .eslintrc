{
    "extends": [
        "react-app",
        "react-app/jest"
      ],
    "rules": {
        "no-console": "warn",
        "quotes": ["error", "double"],
        "jsx-quotes": ["error", "prefer-double"],
        "indent": ["warn", 4],
        "max-len": ["error", {"code": 140}],
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["warn", "always"],
        
    }
}